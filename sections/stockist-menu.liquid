            },
			{
              "type": "text",
              "id": "faq-contact-email",
              "label": "FAQ Contact Email",
              "default": "Email Us"
            },
			{
              "type": "url",
              "id": "faq-contact-email-cta",
              "label": "FAQ Contact Email"
            }            
		],
	"blocks": [
		{
		"type": "question",
        "name": "Question",
        "settings": [
          {
            "type": "text",
            "id": "title",
            "label": "Question",
            "default": "Frequently asked question"
          },
          {
            "type": "richtext",
            "id": "text",
            "label": "Text",
            "default": "<p>Use this text to answer questions in as much detail as possible for your customers.</p>"
          }
        ]
		}
		]
     
  }
{% endschema %}

