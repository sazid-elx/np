<script type="text/javascript">
  
  var $;
(function atPollForjQuery() {
    if (window.jQuery !== undefined) {
        $ = window.jQuery;
        if ($("#az-faq").length > 0) {
            try {
              
              	$(document).ready(function() {
                    $.each($('.faq-menu'), function(){
                        $('#faq-nav > nav').append($(this));
                    });
                    $.each($('.faq-menu-contents'), function(){
                        $('.faq-content').append($(this));
                    });
                                     
                    var url =  window.location.href;
                  var queryString = url ? url.split('?')[1] : window.location.search.slice(1);
                  var obj = {};
                  if (queryString) {
                    queryString = queryString.split('#')[0];
                    var arr = queryString.split('&');

                    for (var i = 0; i < arr.length; i++) {
                        var a = arr[i].split('=');
                        var paramName = a[0];
                        var paramValue = typeof (a[1]) === 'undefined' ? true : a[1];
                        if (paramName.match(/\[(\d+)?\]$/)) {
                          var key = paramName.replace(/\[(\d+)?\]/, '');
                          if (!obj[key]) obj[key] = [];
                          if (paramName.match(/\[\d+\]$/)) {
                            var index = /\[(\d+)\]/.exec(paramName)[1];
                            obj[key][index] = paramValue;
                          } else {
                            obj[key].push(paramValue);
                          }
                        } else {
                          if (!obj[paramName]) {
                            obj[paramName] = paramValue;
                          } else if (obj[paramName] && typeof obj[paramName] === 'string'){
                            obj[paramName] = [obj[paramName]];
                            obj[paramName].push(paramValue);
                          } else {
                            obj[paramName].push(paramValue);
                          }
                        }
                    }

                    if (obj['p']) {
                      var page = obj['p'];
                      setTimeout(function(){
                      $("#faq-nav nav a[href='#"+page+"']").click();
                      return false;
                      }, 1000);
                      
                    }
                  }
                 
                });
              	setTimeout(function(){ 
                   var faqBody = $(".faq-content section:not(:first-child)");
                $(".faq-card > a").on("click", function(event) {
                  event.preventDefault();
                    if ($(this).hasClass("active")) {
                        $(this).removeClass("active");
                        $(this).siblings(".content").slideUp(200);
                        $(".faq-card > a i").removeClass("tab-close").addClass("tab-open");
                    } else {
                        $(".faq-card > a i").removeClass("tab-close").addClass("tab-open");
                        $(this).find("i").removeClass("tab-open").addClass("tab-close");
                        $(".faq-card > a").removeClass("active");
                        $(this).addClass("active");
                        $(".content").slideUp(200);
                        $(this).siblings(".content").slideDown(200);
                    }
                });
                faqBody.css("display", "none");
                	setNav();
              $("#faq-nav nav a").on("click", function(event) {
                    event.preventDefault();
                    var self = $(this);
                    $("#faq-nav nav a").removeClass("active");
                    self.toggleClass("active");
                    faqBody.css("display", "none");
                    $(".faq-content section:not(:first-child) .faq-card a").removeClass("active");
                    $(".faq-content section:not(:first-child) .faq-card a i").removeClass("tab-close").addClass("tab-open");
                    $(".faq-content section:not(:first-child) .faq-card .content").css("display", "none");
                    setNav(self[0].hash);
                });
                   }, 1000);
            } catch (err) {
                // console.log('Try error FAQ section: ' + err);
            }
        } else {
            setTimeout(atPollForjQuery, 25);
        }
    } else {
        setTimeout(atPollForjQuery, 25);
    }
  
})();

function setNav(href) {
    switch (href) {
        case "#faq-second":
            $("#faq-second").css("display", "block");
            break;
        case "#faq-third":
            $("#faq-third").css("display", "block");
            break;
        case "#faq-fourth":
            $("#faq-fourth").css("display", "block");
            break;
        case "#faq-fifth":
            $("#faq-fifth").css("display", "block");
        	$(".faq-card > a i").removeClass("tab-close").addClass("tab-open");
            $('.faq-card > a').find("i").removeClass("tab-open").addClass("tab-close");
            $(".faq-card > a").removeClass("active");
           	$('.faq-card > a').addClass("active");
            $(".content").slideUp(200);
            $('.faq-card > a').siblings(".content").slideDown(200);
            break;
        case "#faq-six":
            $("#faq-six").css("display", "block");
            break;
        case "#faq-seven":
            $("#faq-seven").css("display", "block");
            break;
        case "#faq-eight":
            $("#faq-eight").css("display", "block");
            break;
        case "#faq-nine":
            $("#faq-nine").css("display", "block");
            break;
        case "#faq-ten":
            $("#faq-ten").css("display", "block");
            break;
        case "#faq-first":
        	$("#faq-first").css("display", "block");
            break;
        default:
            if (window.localStorage.getItem("showSizeGuide")) {
              window.localStorage.removeItem("showSizeGuide")
              $("#faq-ten").css("display", "block");
              document.querySelector("#faq-nav nav a").classList.remove("active")
              document.querySelector("a[href='#faq-ten'].faq-menu").classList.add("active")
            }else {
              $("#faq-first").css("display", "block");
            }
            document.querySelector(".faq-page-width.not-loaded").classList.remove("not-loaded")
            document.querySelector("#az-faq").classList.remove("custom-z-index")
//         	$(".faq-card > a i").removeClass("tab-close").addClass("tab-open");
//             $('.faq-card > a').find("i").removeClass("tab-open").addClass("tab-close");
//             $(".faq-card > a").removeClass("active");
//            	$('.faq-card > a').addClass("active");
//             $(".content").slideUp(200);
//             $('.faq-card > a').siblings(".content").slideDown(200);
            break;
    }
}
  const faqTitle = document.querySelector(".for-mobile .custom-faq-title")
  const sizeGuideTitle = document.querySelector(".for-mobile .custom-page-title.for-mobile-size-guide")
  const firstElem = document.querySelector('#faq-nav nav a[href="#faq-first"]')
  const sideLinks = document.querySelectorAll('.faq-menu');
  if (window.innerWidth <= 768) {
    sideLinks.forEach(item=>{
      item.addEventListener('click', (e)=>{
        if (e.target.href=='https://ninetypercent.com/pages/new-faq#faq-ten'){
          sizeGuideTitle.classList.remove('hide')
          faqTitle.classList.add('hide')
        }
        else{
          if (!sizeGuideTitle.classList.contains('hide')){
            sizeGuideTitle.classList.add('hide')
          }
          faqTitle.classList.remove('hide')
        }
      })
    })
    firstElem.addEventListener('click', ()=>{
        if (!sizeGuideTitle.classList.contains('hide')){
            sizeGuideTitle.classList.add('hide')
          }
        faqTitle.classList.remove('hide')
    })
  }
  
  
 </script>



